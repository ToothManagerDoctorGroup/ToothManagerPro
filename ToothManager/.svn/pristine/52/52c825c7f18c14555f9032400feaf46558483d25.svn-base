#import <UIKit/UIKit.h>
/**
 *  选择图片的展示视图
 */
@interface MCPhotoPickerView : UIView
/**
 *  存取照片数组,类型存为UIImage
 */
@property (nonatomic, strong) NSMutableArray *photoArray;
/**
 *  列数
 */
@property (nonatomic, assign) NSUInteger columns;
/**
 *  最大张数
 */
@property (nonatomic, assign) NSUInteger maxPhotos;
/**
 *  刷新视图
 */
- (void)reloadView;
@end
